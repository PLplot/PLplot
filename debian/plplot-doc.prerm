#! /bin/sh
# prerm script for plplot-doc

set -e

case "$1" in
    remove|upgrade|deconfigure)
       install-info --quiet --remove /usr/share/info/plplotdoc.info*
        ;;
    failed-upgrade)
        ;;
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

#DEBHELPER#

exit 0


