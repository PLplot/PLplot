  [24,  1, 1, "
<p>Credits:
<ul>
<li><a href=\\\"http://www.columbia.edu/~fdc/pace/\\\">Say Peace in all languages!</a>
<li><a href=\\\"http://www.columbia.edu/~fdc/pace/flag.html\\\">Pace flag</a>
</ul>
"],
  [25,  4, 1, ""],
  [26,  2, 1, ""],
  [27, 10, 1, ""],
  [28,  5, 1, ""]);

$count = 0;

foreach $drec (@demos) {

  $demo = sprintf ("%02d", $drec->[0]);
  $dir = "demo$demo";
  $file = "$dir.php";
  $thumbnail = sprintf ("%02d", $drec->[2]);
  $npages = $drec->[1];
  $credits = $drec->[3];

  open (PHP, "> $file")
    or die "Cannot create $file";
    
  print PHP qq{<?php
include ("example_page.php");
demo_page ($drec->[0], $npages, \$lbind, "$credits");
?>
};

  close PHP;

  print "
    <tr>" if ($count % 3 == 0);
_:>
      <td> <A HREF="<:=$file:>"><IMG BORDER="0" SRC="../examples-data/<:=$dir:>/prev-x<:=$demo:>.<:=$thumbnail:>.png"></a> </td><:
print "
    </tr>
" if ($count % 3 == 2 or $count == scalar (@demos) - 1);

  $count++;
}:>

