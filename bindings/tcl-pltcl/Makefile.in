Pldriver_SOURCES = \
../../drivers/dg300.c ../../drivers/hpgl.c ../../drivers/impress.c \
../../drivers/linuxvga.c ../../drivers/ljii.c ../../drivers/ljiip.c \
../../drivers/mgr.c ../../drivers/next.c ../../drivers/null.c \
../../drivers/pbm.c ../../drivers/plbuf.c ../../drivers/plmeta.c \
../../drivers/ps.c ../../drivers/tek.c ../../drivers/tk.c \
../../drivers/xfig.c ../../drivers/xwin.c

Plcore_SOURCES = \
../../src/pdfutils.c ../../src/plargs.c ../../src/plbox.c \
../../src/plcont.c ../../src/plcore.c ../../src/plctrl.c \
../../src/plcvt.c ../../src/pldtik.c ../../src/plfill.c \
../../src/plhist.c ../../src/plline.c ../../src/plmap.c \
../../src/plot3d.c ../../src/plpage.c ../../src/plsdef.c \
../../src/plshade.c ../../src/plstripc.c ../../src/plsym.c \
