#========================================================================
# Enumerate the names of the object files included in this package.
# These objects are created and linked into the final library.  In
# most cases these object files will correspond to the source files
# above.
#
#========================================================================

Pltcl_OBJECTS =  \
tclAPI.$(OBJEXT) ckcalloc.$(OBJEXT) \
pdfutils.$(OBJEXT) plargs.$(OBJEXT) plbox.$(OBJEXT) \
plcont.$(OBJEXT) plcore.$(OBJEXT) plctrl.$(OBJEXT) \
plcvt.$(OBJEXT) pldtik.$(OBJEXT) plfill.$(OBJEXT) \
plhist.$(OBJEXT) plline.$(OBJEXT) plmap.$(OBJEXT) \
plot3d.$(OBJEXT) plpage.$(OBJEXT) plsdef.$(OBJEXT) \
plshade.$(OBJEXT) plstripc.$(OBJEXT) plsym.$(OBJEXT) \
pltick.$(OBJEXT) plvpor.$(OBJEXT) plwind.$(OBJEXT) \
dg300.$(OBJEXT) hpgl.$(OBJEXT) impress.$(OBJEXT) \
linuxvga.$(OBJEXT) ljii.$(OBJEXT) ljiip.$(OBJEXT) \
